{
    "log": {
        "disabled": false,
        "level": "error",
        "output": "./box.log",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "dns_proxy",
                "address": "https://1.0.0.1/dns-query",
                "detour": "国外"
            },
            {
                "tag": "dns_global",
                "address": "https://1.0.0.1/dns-query",
                "detour": "全局"
            },
            {
                "tag": "dns_local",
                "address": "local",
                "detour": "direct"
            },
            {
                "tag": "nx",
                "address": "rcode://success"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "dns_local"
            },
            {
                "rule_set": "category-ads-all",
                "server": "nx",
                "rewrite_ttl": 0,
                "disable_cache": true
            },
            {
                "clash_mode": "Direct",
                "server": "dns_local"
            },
            {
                "clash_mode": "Global",
                "server": "dns_global"
            },
            {
                "rule_set": [
                    "Skip-https",
                    "Push"
                ],
                "server": "dns_local"
            },
            {
                "rule_set": "Direct-domain",
                "server": "dns_local"
            },
            {
                "rule_set": [
                    "geosite-tiktok@!cn",
                    "Proxy-domain"
                ],
                "server": "dns_proxy"
            },
            {
                "rule_set": [
                    "geosite-cn",
                    "geosite-bytedance",
                    "geosite-apple@cn",
                    "geosite-microsoft@cn",
                    "geosite-category-games@cn",
                    "geosite-category-companies@cn"
                ],
                "server": "dns_local"
            },
            {
                "rule_set": "geosite-geolocation-!cn",
                "server": "dns_proxy"
            }
        ],
        "final": "dns_proxy",
        "reverse_mapping": true,
        "strategy": "prefer_ipv4",
        "independent_cache": true
    },
    "inbounds": [
        {
            "type": "tun",
            "tag": "tun-in",
            "interface_name": "tun0",
            "address": [
                "172.18.0.1/30",
                "fdfe:dcba:9876::1/126"
            ],
            "strict_route": true,
            "auto_route": true,
            "stack": "mixed",
            "sniff": true
        }
    ],
    "outbounds": [
        {
            "type": "dns",
            "tag": "dns"
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        },
        {
            "type": "selector",
            "tag": "国外",
            "outbounds": ""
        },
        {
            "type": "selector",
            "tag": "国内",
            "outbounds": [
                "direct"
            ]
        },
        {
            "type": "selector",
            "tag": "全局",
            "outbounds": ""
        }
    ],
    "route": {
        "rule_set": [
            {
                "type": "remote",
                "tag": "geosite-geolocation-!cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-geolocation-!cn.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "category-ads-all",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Yuu518/sing-box-rules/rule_set/rule_set_site/category-ads-all.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "geosite-tiktok@!cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-tiktok@!cn.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "geosite-bytedance",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-bytedance.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "geosite-apple@cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-apple@cn.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "geosite-microsoft@cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-microsoft@cn.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "geosite-category-games@cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-category-games@cn.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "geoip-cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/cn.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "geosite-cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-cn.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "geosite-category-companies@cn",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/CHIZI-0618/v2ray-rules-dat/release/singbox_rule_set/geosite-category-companies@cn.srs",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "Proxy-domain",
                "url": "https://raw.githubusercontent.com/eiaousa/sb_rule_set/refs/heads/main/Proxy-domain.json",
                "format": "source",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "Direct-domain",
                "url": "https://raw.githubusercontent.com/eiaousa/sb_rule_set/refs/heads/main/Direct-domain.json",
                "format": "source",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "Push",
                "url": "https://raw.githubusercontent.com/eiaousa/sb_rule_set/refs/heads/main/Push.json",
                "format": "source",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "Puship",
                "url": "https://raw.githubusercontent.com/eiaousa/sb_rule_set/refs/heads/main/Puship.json",
                "format": "source",
                "download_detour": "国外",
                "update_interval": "24h"
            },
            {
                "type": "remote",
                "tag": "Skip-https",
                "url": "https://raw.githubusercontent.com/eiaousa/sb_rule_set/refs/heads/main/Skip-https.json",
                "format": "source",
                "download_detour": "国外",
                "update_interval": "24h"
            }
        ],
        "rules": [
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "protocol": "dns"
                    },
                    {
                        "port": 53
                    }
                ],
                "outbound": "dns"
            },
            {
                "clash_mode": "Direct",
                "outbound": "direct"
            },
            {
                "clash_mode": "Global",
                "outbound": "全局"
            },
            {
                "port": 853,
                "network": "tcp",
                "outbound": "block"
            },
            {
                "port": 443,
                "network": "udp",
                "outbound": "block"
            },
            {
                "rule_set": [
                    "Skip-https",
                    "Push"
                ],
                "outbound": "direct"
            },
            {
                "rule_set": "Direct-domain",
                "outbound": "国内"
            },
            {
                "rule_set": [
                    "geosite-tiktok@!cn",
                    "Proxy-domain"
                ],
                "outbound": "国外"
            },
            {
                "rule_set": [
                    "geosite-cn",
                    "geosite-bytedance",
                    "geosite-apple@cn",
                    "geosite-microsoft@cn",
                    "geosite-category-games@cn",
                    "geosite-category-companies@cn"
                ],
                "outbound": "国内"
            },
            {
                "rule_set": "geosite-geolocation-!cn",
                "outbound": "国外"
            },
            {
                "rule_set": "Puship",
                "outbound": "direct"
            },
            {
                "rule_set": "geoip-cn",
                "outbound": "国内"
            },
            {
                "ip_cidr": [
                    "0.0.0.0/8",
                    "127.0.0.0/8",
                    "192.168.0.0/16",
                    "::/128",
                    "::1/128",
                    "fc70::/10",
                    "fe80::/10"
                ],
                "outbound": "direct"
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            },
            {
                "package_name": [
                    "com.blueocean.musicplayer"
                ],
                "outbound": "国内"
            },
            {
                "rule_set": "category-ads-all",
                "outbound": "block"
            }
        ],
        "final": "国外",
        "find_process": true,
        "auto_detect_interface": true
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "path": "cache.db"
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui_download_detour": "国外",
            "external_ui": "ui"
        }
    }
}