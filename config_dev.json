                "format": "source",
                "download_detour": "🔴国外出站"
            },
            {
                "type": "remote",
                "tag": "Puship",
                "url": "https://raw.githubusercontent.com/eiaousa/sb_rule_set/refs/heads/main/Puship.json",
                "format": "source",
                "download_detour": "🔴国外出站"
            },
            {
                "type": "remote",
                "tag": "Skip-https",
                "url": "https://raw.githubusercontent.com/eiaousa/sb_rule_set/refs/heads/main/Skip-https.json",
